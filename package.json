{
  "name": "@elizaos/eliza-starter",
  "version": "0.25.9",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "start": "tsc && node --loader ts-node/esm src/index.ts",
    "clean": "./scripts/clean.sh",
    "start:service:sei": "pm2 start pnpm --name=\"sei\" --restart-delay=3000 --max-restarts=10 -- run start --character=\"./characters/sei.character.json\"",
    "stop:service:sei": "pm2 stop sei"
  },
  "dependencies": {
    "@duneanalytics/client-sdk": "0.2.5",
    "@elizaos/adapter-postgres": "0.25.6-alpha.1",
    "@elizaos/adapter-sqlite": "0.25.6-alpha.1",
    "@elizaos/client-direct": "0.25.9",
    "@elizaos/core": "0.25.9",
    "@elizaos/plugin-bootstrap": "0.25.9",
    "agent-twitter-client": "github:timmyg/agent-twitter-client#main",
    "amqplib": "0.10.5",
    "better-sqlite3": "11.8.1",
    "discord.js": "14.16.3",
    "fs": "0.0.1-security",
    "glob": "11.0.0",
    "net": "1.0.2",
    "openai": "4.86.2",
    "path": "0.12.7",
    "readline": "1.3.0",
    "url": "0.11.4",
    "whatwg-url": "7.1.0",
    "ws": "8.18.0",
    "yargs": "17.7.2",
    "zod": "3.24.2"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "1.20.0",
      "@elizaos/core": "0.25.9"
    },
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild",
      "onnxruntime-node"
    ]
  },
  "devDependencies": {
    "pm2": "5.4.3",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "typescript": "5.6.3"
  }
}
